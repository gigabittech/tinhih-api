                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Logged in successfully"
                                        },
                                        "token": {
                                            "type": "string",
                                            "example": "your-generated-token"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
